{
  "name": "easy-finances",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:href": "ng build --base-href /EasyFinancesApp/",
    "build:github": "npm run delete:docs && npm run build:href && npm run copy:dist:browser",
    "build:github:win": "npm run delete:docs:win && npm run build:href && npm run copy:dist:browser",
    "coverage": "nyc ng test --watch=false",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --code-coverage --coverageReporters=lcov",
    "delete:docs": "rm -rf docs",
    "delete:docs:win": "del docs",
    "copy:dist:browser": "copyfiles dist/easy-finances/browser/* ./docs -f"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.9",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "bootstrap": "^5.3.3",
    "chart.js": "^3.9.1",
    "jwt-decode": "^3.1.2",
    "ng2-charts": "^4.0.1",
    "ng2-currency-mask": "^13.0.3",
    "ngx-toastr": "^19.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-builders/jest": "^18.0.0",
    "@angular-devkit/build-angular": "^18.2.10",
    "@angular/cli": "^18.2.3",
    "@angular/compiler-cli": "^18.2.0",
    "@types/bootstrap": "^5.2.10",
    "@types/jasmine": "~5.1.0",
    "@types/uuid": "^10.0.0",
    "copyfiles": "^2.4.1",
    "jasmine-core": "^4.6.1",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^5.0.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "nyc": "^17.1.0",
    "typescript": "~5.5.2"
  }
}
